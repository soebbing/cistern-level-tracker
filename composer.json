{
    "name": "soebbing/cistern-level-tracker",
    "description": "A little example of storing data of a cistern sensor",
    "type": "project",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": ">=8",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.13",
        "easycorp/easyadmin-bundle": "^3.5",
        "sensio/framework-extra-bundle": "^5.6",
        "symfony/console": "5.4.*",
        "symfony/dotenv": "5.4.*",
        "symfony/flex": "1.19.*",
        "symfony/framework-bundle": "5.4.*",
        "symfony/intl": "5.4.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/runtime": "5.4.*",
        "symfony/translation": "5.4.*",
        "symfony/twig-bundle": "5.4.*",
        "symfony/yaml": "5.4.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^3.4"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.7",
        "doctrine/doctrine-fixtures-bundle": "^3.4.0",
        "friendsofphp/php-cs-fixer": "v3.0.*",
        "liip/test-fixtures-bundle": "2.4.*",
        "php-parallel-lint/php-parallel-lint": "@stable",
        "php-parallel-lint/php-var-dump-check": "@stable",
        "phpmd/phpmd": "^2.13",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7",
        "symfony/browser-kit": "5.4.*",
        "symfony/css-selector": "5.4.*",
        "symfony/debug-bundle": "^5.2",
        "symfony/maker-bundle": "^1.36",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/stopwatch": "5.4.*",
        "symfony/web-profiler-bundle": "5.4.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "exclude-from-classmap": [
            "/src/Migrations/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test-coverage": "vendor/bin/phpunit --bootstrap tests/bootstrap.php  --coverage-text --coverage-html var/coverage",
        "test": "vendor/bin/phpunit -v --bootstrap tests/bootstrap.php ",
        "cs-fix": "vendor/bin/php-cs-fixer fix --allow-risky yes",
        "cs": "vendor/bin/php-cs-fixer fix --dry-run --allow-risky=yes",
        "phpstan": "vendor/bin/phpstan analyse src",
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*"
        }
    }
}
